{
  "name": "where-did-the-time-go",
  "version": "1.0.0",
  "description": "Obsidian plugin to track where your time went",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",

    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^16.11.6",
    "builtin-modules": "^5.0.0",
    "esbuild": "0.17.3",
    "jest": "^29.7.0",
    "obsidian": "^1.7.2",
    "ts-jest": "^29.4.6",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  }
}
